<p id="notice"><%= notice %></p>

<h1>Listing Stagelevels</h1>

<table>
  <thead>
    <tr>
      <th>Order</th>
      <th>Ammount</th>
      <th>Rate</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stagelevels.each do |stagelevel| %>
      <tr>
        <td><%= stagelevel.order %></td>
        <td><%= stagelevel.ammount %></td>
        <td><%= stagelevel.rate %></td>
        <td><%= link_to 'Show', stagelevel %></td>
        <td><%= link_to 'Edit', edit_stagelevel_path(stagelevel) %></td>
        <td><%= link_to 'Destroy', stagelevel, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Stagelevel', new_stagelevel_path %>
