
<div class="ibox-content form-horizontal m-t">


  <div class="form-group">
    <%= form_for(@peo,:html => {:class=>'form-horizontal m-t',:multipart => true}) do |f| %>

        <div class="form-group">
          <label class="col-sm-3 control-label">会员等级：</label>
          <div class="col-sm-6">
            <%= f.text_field :memberlevel,class:"form-control" %>
          </div>
        </div>

        <div class="form-group">
          <label class="col-sm-3 control-label">会员名：</label>
          <div class="col-sm-6">
            <%= f.text_field :name,class:"form-control" %>
          </div>
        </div>

        <div class="form-group">
          <label class="col-sm-3 control-label">信用额度：</label>
          <div class="col-sm-6">
            <%= f.text_field :credit,class:"form-control" %>
          </div>
        </div>

        <div class="form-group">
          <label class="col-sm-3 control-label">性别：</label>
          <div class="col-sm-6">
            <%= f.text_field :sex,class:"form-control" %>
          </div>
        </div>

        <div class="form-group">
          <label class="col-sm-3 control-label">电话：</label>
          <div class="col-sm-6">
            <%= f.text_field :tel,class:"form-control" %>
          </div>
        </div>

        <div class="form-group">
          <label class="col-sm-3 control-label">银行卡：</label>
          <div class="col-sm-6">
            <%= f.text_field :bankcard,class:"form-control" %>
          </div>
        </div>


        <div class="form-group">
          <label class="col-sm-3 control-label">银行卡号：</label>
          <div class="col-sm-6">
            <%= f.text_field :bankaccount,class:"form-control" %>
          </div>
        </div>

        <div class="form-group">
          <label class="col-sm-3 control-label">紧急联系人：</label>
          <div class="col-sm-6">
            <%= f.text_field :contact,class:"form-control" %>
          </div>
        </div>

        <div class="form-group">
          <label class="col-sm-3 control-label">紧急联系人性别：</label>
          <div class="col-sm-6">
            <%= f.text_field :contactsex,class:"form-control" %>
          </div>
        </div>

        <div class="form-group">
          <label class="col-sm-3 control-label">紧急联系人电话：</label>
          <div class="col-sm-6">
            <%= f.text_field :contacttel,class:"form-control" %>
          </div>
        </div>

        <div class="form-group">
          <label class="col-sm-3 control-label">营业执照：</label>
          <div class="col-sm-6">
            <%= f.text_field :businesslicense,class:"form-control" %>
          </div>
        </div>

        <div class="form-group">
          <label class="col-sm-3 control-label">QR码：</label>
          <div class="col-sm-6">
            <%= f.text_field :qrcode,class:"form-control" %>
          </div>
        </div>

        <div class="form-group">
          <label class="col-sm-3 control-label">单据：</label>
          <div class="col-sm-6">
            <%= f.text_field :upid,class:"form-control" %>
          </div>
        </div>

        <div class="form-group">
          <label class="col-sm-3 control-label">状态：</label>
          <div class="col-sm-6">
            <%= f.text_field :status,class:"form-control" %>
          </div>
        </div>

        <div class="form-group">
          <label class="col-sm-3 control-label">微信：</label>
          <div class="col-sm-6">
            <%= f.text_field :wechat,class:"form-control" %>
          </div>
        </div>

        <div class="actions">
          <%= f.submit '确定',class: "btn btn-w-m btn-success mylink pull-right",'data-no-turbolink'=>true %>

    <% end %>
    </div>
  </div>
</div>
