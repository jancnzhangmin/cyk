


<div class="ibox-content form-horizontal m-t">


  <div class="form-group">
    <%= form_for(@creditdetail,:html => {:class=>'form-horizontal m-t',:multipart => true}) do |f| %>

        <div class="form-group">
          <label class="col-sm-3 control-label">征信数量：</label>
          <div class="col-sm-6">
            <%= f.text_field :amount,class:"form-control" %>
          </div>
        </div>

        <div class="form-group">
          <label class="col-sm-3 control-label">征信日期：</label>
          <div class="col-sm-6">
            <%= f.text_field :amountdate,class:"form-control" %>
          </div>
        </div>

        <div class="actions">
          <%= f.submit '确定',class: "btn btn-w-m btn-success mylink pull-right",'data-no-turbolink'=>true %>

        <% end %>
        </div>
  </div>
</div>

