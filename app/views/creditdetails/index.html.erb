
<%= render 'left' %>
<%= render 'top' %>
<div class="col-lg-12">
  <div class="ibox float-e-margins">
    <div class="ibox-title">
      <h5>征信列表</h5>
    </div>
    <div class="ibox-content">

      <table class="table table-hover table-bordered">
        <thead>
        <tr>
          <th>征信数量</th>
          <th>征信日期</th>
          <th colspan="2"></th>
        </tr>
        </thead>

        <tbody>
        <% @creditdetails.each do |creditdetail| %>
            <tr>
              <td><%= creditdetail.amount %></td>
              <td><%= creditdetail.amountdate %></td>
              <td><%= link_to '编辑征信', edit_creditdetail_path(creditdetail) %></td>
              <td><%= link_to '删除征信', creditdetail, method: :delete, data: { confirm: '确定删除?' } %></td>
            </tr>
        <% end %>
        </tbody>
      </table>


      <%= link_to "新增", new_creditdetail_path,class: "btn btn-w-m btn-success mylink pull-right",'data-no-turbolink'=>true %>
      <br>
    </div>

  </div>
</div>
